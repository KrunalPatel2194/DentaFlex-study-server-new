{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "methods": ["OPTIONS"],
        "dest": "/app.js",
        "headers": {
          "Access-Control-Allow-Origin": "https://dentaflex-study-admin-portal.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization",
          "Access-Control-Allow-Credentials": "true"
        },
        "status": 204
      },
      {
        "src": "/(.*)",
        "dest": "/app.js",
        "headers": {
          "Access-Control-Allow-Origin": "https://dentaflex-study-admin-portal.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization",
          "Access-Control-Allow-Credentials": "true"
        }
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }